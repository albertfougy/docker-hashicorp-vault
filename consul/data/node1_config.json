{
  "addresses" : {
    "https" : "0.0.0.0"
  },
  "bootstrap_expect": 3,
  "bind_addr": "0.0.0.0",
  "check_update_interval": "5m",
  "client_addr": "0.0.0.0",
  "datacenter": "docker_dc",
  "data_dir": "/tmp/consul",
  "disable_remote_exec": true,
  "disable_update_check": true,
  "encrypt": "cg8StVXbQJ0gPvMd9o7yrg==",
  "ca_file": "/data/certificates/consul-root.cer",
  "cert_file": "/data/certificates/consul-server.cer",
  "key_file": "/data/certificates/consul-server.key",
  "verify_outgoing" : true,
  "verify_incoming_rpc" : true,
  "verify_incoming_https" : false,
  "verify_server_hostname" : true,
  "encrypt_verify_incoming" : true,
  "encrypt_verify_outgoing" : true,
  "http_config": {
    "response_headers": {
      "Access-Control-Allow-Origin": "*"
    }
  },
  "leave_on_terminate" : true,
  "node_name" : "node1",
  "node_meta": {
      "instance_type": "Docker container"
  },
  "ports" : {
    "https" : 8700
  },
  "retry_join" : [ "consul.2.container:8301", "consul.3.container:8301" ],
  "server" : true,
  "server_name" : "node1",
  "ui" : true
}
